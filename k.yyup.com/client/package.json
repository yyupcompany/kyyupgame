{
  "name": "kindergarten-client",
  "version": "1.0.0",
  "description": "幼儿园管理系统前端",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/kill-ports.js && vite --host 0.0.0.0 --port 5173",
    "dev:fast:simple": "vite --host 0.0.0.0 --port 5173 --force",
    "dev:bash": "bash scripts/kill-ports.sh && vite --host 0.0.0.0 --port 5173",
    "dev:win": "scripts\\kill-ports-simple.bat && vite --host 0.0.0.0 --port 5173",
    "dev:cmd": "call scripts\\kill-ports-simple.bat && vite --host 0.0.0.0 --port 5173",
    "dev:cross": "node scripts/kill-ports.js && vite --host 0.0.0.0 --port 5173",
    "dev:turbo": "bash scripts/kill-ports.sh && NODE_OPTIONS='--max-old-space-size=51200' UV_THREADPOOL_SIZE=50 ESBUILD_WORKER_THREADS=50 vite --host 0.0.0.0 --port 5173 --force",
    "dev:safe": "node scripts/dev-with-error-handling.cjs",
    "dev:vite": "bash scripts/kill-ports.sh && vite --host --port 5173",
    "dev:force": "bash scripts/kill-ports.sh && vite --host --force --port 5173",
    "dev:legacy": "bash scripts/kill-ports.sh && node scripts/dev-server.js",
    "dev:wsl": "bash scripts/kill-ports.sh && bash scripts/start-wsl-dev.sh",
    "dev:fast": "bash scripts/kill-ports.sh && NODE_OPTIONS='--max-old-space-size=12288' UV_THREADPOOL_SIZE=12 vite --config vite.config.wsl.ts --host --port 5173",
    "dev:localhost": "bash scripts/kill-ports.sh && export no_proxy=localhost,127.0.0.1,::1 && vite --config vite.config.k-yyup.ts --host 0.0.0.0 --port 5173 --force",
    "build": "vite build",
    "build:prod": "vite build --config vite.config.prod.ts --mode production",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=51200' UV_THREADPOOL_SIZE=50 ESBUILD_WORKER_THREADS=50 vite build",
    "build:turbo": "NODE_OPTIONS='--max-old-space-size=51200' UV_THREADPOOL_SIZE=50 ESBUILD_WORKER_THREADS=50 vite build --config vite.config.turbo.ts --mode production",
    "build:ultra": "NODE_OPTIONS='--max-old-space-size=51200 --max-semi-space-size=128 --max-old-space-size=51200' UV_THREADPOOL_SIZE=50 ESBUILD_WORKER_THREADS=50 NODE_ENV=production vite build --config vite.config.turbo.ts --mode production --logLevel error",
    "typecheck": "tsc --noEmit --pretty",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report test-results/e2e-report",
    "test:e2e:auth": "playwright test tests/e2e/auth.e2e.test.ts",
    "test:e2e:user-management": "playwright test tests/e2e/user-management.e2e.test.ts",
    "test:e2e:ai-assistant": "playwright test tests/e2e/ai-assistant.e2e.test.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:real-integration": "node tests/run-real-integration-tests.js",
    "test:real-backend": "vitest run tests/integration/real-backend.test.ts",
    "test:database-transaction": "vitest run tests/integration/database-transaction.test.ts",
    "test:user-workflow": "vitest run tests/integration/user-workflow.test.ts",
    "test:multi-user-concurrent": "vitest run tests/integration/multi-user-concurrent.test.ts",
    "test:performance-integration": "vitest run tests/integration/performance-integration.test.ts",
    "test:environment-consistency": "vitest run tests/environment/production-consistency.test.ts",
    "test:e2e-real-world": "playwright test tests/e2e/real-world-scenarios.test.ts",
    "check:completeness": "node scripts/check-development-completeness.js",
    "check:completeness:sh": "bash scripts/check-completeness.sh",
    "check:quick": "node scripts/quick-check.js",
    "check:accurate": "node scripts/accurate-test-scanner.cjs",
    "lint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint src --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "npx vite-bundle-analyzer dist",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:serve": "npx http-server docs -p 8080 -o",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test:api": "node --loader ts-node/esm tests/api/runTests.ts",
    "test:api:report": "ts-node --esm tests/api/runTests.ts --report",
    "test:api:help": "ts-node --esm tests/api/runTests.ts --help",
    "test:api:fix": "ts-node --esm tests/api/fixFrontend.ts",
    "logs": "node scripts/view-logs.js",
    "logs:access": "node scripts/view-logs.js access",
    "logs:vite": "node scripts/view-logs.js vite",
    "logs:error": "node scripts/view-logs.js error",
    "logs:follow": "node scripts/view-logs.js vite --follow",
    "logs:all": "node scripts/view-logs.js all",
    "tauri": "npx tauri",
    "tauri:dev": "npx tauri dev",
    "tauri:build": "npx tauri build",
    "test:sidebar": "node run-sidebar-tests.cjs",
    "test:sidebar:centers": "node run-sidebar-tests.cjs centers",
    "test:sidebar:teacher": "node run-sidebar-tests.cjs teacher",
    "test:sidebar:parent": "node run-sidebar-tests.cjs parent",
    "test:sidebar:help": "node run-sidebar-tests.cjs help",
    "test:sidebar:report": "node sidebar-test-manager.cjs",
    "test:sidebar:check": "node test-sidebar-setup.cjs"
  },
  "dependencies": {
    "@ant-design/web3": "^1.24.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@types/qrcode": "^1.5.6",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vueuse/core": "^13.3.0",
    "antd": "^5.25.4",
    "axios": "1.6.7",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "echarts": "^5.6.0",
    "element-plus": "^2.2.16",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "html2pdf.js": "^0.12.1",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.542.0",
    "marked": "^15.0.12",
    "mysql2": "^3.14.1",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "pinia": "^2.0.22",
    "playwright": "^1.56.1",
    "qrcode": "^1.5.4",
    "uuid": "^11.1.0",
    "vant": "^4.9.21",
    "vue": "^3.5.16",
    "vue-router": "^4.1.5",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.56.1",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.118",
    "@types/path-browserify": "^1.0.3",
    "@types/uuid": "^10.0.0",
    "@types/vue": "^1.0.31",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.3",
    "@vue/compiler-sfc": "^3.5.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^1.22.0",
    "chalk": "^5.4.1",
    "glob": "^10.4.5",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.5",
    "postcss-px-to-viewport": "^1.1.1",
    "puppeteer": "^24.11.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.54.9",
    "terser": "^5.46.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^4.5.14",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.0",
    "vue-tsc": "^0.40.13"
  }
}
