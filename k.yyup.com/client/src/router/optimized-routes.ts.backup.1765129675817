  {
    path: '/test/student-management',
    name: 'StudentManagementTest',
    component: () => Promise.resolve({
      default: {
        name: 'StudentManagementTest',
        template: `
          <div style="padding: 20px;">
            <h1>å­¦ç”Ÿç®¡ç†åŠŸèƒ½æµ‹è¯•</h1>
            <p>è¿™æ˜¯ä¸€ä¸ªç®€å•çš„å­¦ç”Ÿç®¡ç†åŠŸèƒ½æ¼”ç¤ºé¡µé¢</p>
            <button @click="loadStudents" :disabled="loading" style="padding: 8px 16px; margin: 10px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer;">
              {{ loading ? 'åŠ è½½ä¸­...' : 'åŠ è½½å­¦ç”Ÿåˆ—è¡¨' }}
            </button>
            <div v-if="error" style="color: red; margin: 10px 0; padding: 10px; background: #f8d7da; border-radius: 4px;">
              é”™è¯¯ï¼š{{ error }}
            </div>
            <div v-if="students.length > 0" style="margin-top: 20px;">
              <h2>å­¦ç”Ÿåˆ—è¡¨ (å…± {{ total }} äºº)</h2>
              <table style="border-collapse: collapse; width: 100%; margin-top: 10px;">
                <thead>
                  <tr style="background: #f8f9fa;">
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">å­¦å·</th>
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">å§“å</th>
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">æ€§åˆ«</th>
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">å¹´é¾„</th>
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">ç­çº§</th>
                    <th style="border: 1px solid #ddd; padding: 8px; text-align: left;">çŠ¶æ€</th>
                  </tr>
                </thead>
                <tbody>
                  <tr v-for="student in students" :key="student.id">
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.studentId }}</td>
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.name }}</td>
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.gender }}</td>
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.age }}</td>
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.className }}</td>
                    <td style="border: 1px solid #ddd; padding: 8px;">{{ student.status }}</td>
                  </tr>
                </tbody>
              </table>
            </div>
            <div v-else-if="!loading && !error" style="margin-top: 20px; color: #666;">
              æš‚æ— å­¦ç”Ÿæ•°æ®ï¼Œè¯·ç‚¹å‡»"åŠ è½½å­¦ç”Ÿåˆ—è¡¨"æŒ‰é’®
            </div>
          </div>
        `,
        data() {
          return {
            students: [],
            total: 0,
            loading: false,
            error: ''
          }
        },
        methods: {
          async loadStudents() {
            (this as any).loading = true;
            (this as any).error = '';

            try {
              const response = await fetch('http://localhost:3000/api/students/by-class?classId=1&page=1&pageSize=20', {
                method: 'GET',
                headers: {
                  'Authorization': 'Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MSwidXNlcm5hbWUiOiJhZG1pbiIsInJvbGUiOiJhZG1pbiIsImlhdCI6MTczNDI1NzUzOSwiZXhwIjoxNzM0MzQzOTM5fQ.Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8Ej8',
                  'Content-Type': 'application/json'
                }
              })

              const data = await response.json()

              if (data.success) {
                (this as any).students = data.data.list;
                (this as any).total = data.data.total;
              } else {
                (this as any).error = data.message || 'åŠ è½½å¤±è´¥';
              }
            } catch (err) {
              (this as any).error = 'ç½‘ç»œé”™è¯¯ï¼š' + (err as Error).message;
            } finally {
              (this as any).loading = false;
            }
          }
        },
        mounted() {
          (this as any).loadStudents()
        }
      }
    }),
    meta: {
      title: 'å­¦ç”Ÿç®¡ç†æµ‹è¯•',
      requiresAuth: false,
      hideInMenu: true,
      priority: 'low'
    }
  },

