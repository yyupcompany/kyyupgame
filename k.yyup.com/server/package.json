    "exceljs": "^4.4.0",
    "express": "^4.17.1",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "glob": "^11.0.3",
    "helmet": "^4.6.0",
    "inquirer": "^8.2.6",
    "ioredis": "^5.8.0",
    "joi": "^17.13.3",
    "joi-to-typescript": "^4.15.0",
    "jsdom": "^27.4.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "knex": "^3.1.0",
    "magic-string": "^0.30.17",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "ora": "^5.4.1",
    "os-utils": "^0.0.14",
    "pdfkit": "^0.17.2",
    "pptxgenjs": "^4.0.1",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "scrypt-js": "^3.0.1",
    "sequelize": "^6.12.0-beta.3",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "sortablejs": "^1.15.6",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.7",
    "tmp": "^0.2.3",
    "uuid": "^8.3.2",
    "wave-resampler": "^1.0.0",
    "ws": "^8.18.3",
