    "@types/inquirer": "^8.2.11",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^16.11.12",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/systeminformation": "^3.23.1",
    "@types/ws": "^8.18.1",
    "@volcengine/openapi": "^1.32.0",
    "alawmulaw": "^6.0.0",
    "ali-oss": "^6.23.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
