    "@types/dompurify": "^3.0.5",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.1",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/scrypt-js": "^2.0.4",
    "@types/supertest": "^6.0.3",
    "@types/tmp": "^0.2.6",
