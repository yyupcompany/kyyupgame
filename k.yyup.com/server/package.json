{
  "name": "kindergarten-server",
  "version": "1.0.0",
  "description": "幼儿园管理系统后端服务",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "node scripts/kill-ports.js && cross-env NODE_ENV=development PORT=3000 NODE_OPTIONS=\"--inspect=127.0.0.1:9341 --enable-source-maps\" ts-node --transpile-only src/app.ts",
    "pm2:start": "pm2 start npm --name kyyup-server -- run dev",
    "pm2:restart": "pm2 restart kyyup-server",
    "pm2:stop": "pm2 stop kyyup-server",
    "pm2:delete": "pm2 delete kyyup-server",
    "pm2:logs": "pm2 logs kyyup-server --lines 50",
    "pm2:status": "pm2 status",
    "dev:fast": "cross-env NODE_ENV=development PORT=3000 ts-node src/app.ts",
    "dev:bash": "bash scripts/kill-ports.sh && cross-env NODE_ENV=development PORT=3000 NODE_OPTIONS=\"--inspect=127.0.0.1:9341 --enable-source-maps\" ts-node src/app.ts",
    "dev:win": "scripts\\kill-ports.bat && cross-env NODE_ENV=development PORT=3000 NODE_OPTIONS=\"--inspect=127.0.0.1:9341 --enable-source-maps\" ts-node src/app.ts",
    "dev:cmd": "call scripts\\kill-ports.bat && cross-env NODE_ENV=development PORT=3000 NODE_OPTIONS=\"--inspect=127.0.0.1:9341 --enable-source-maps\" ts-node src/app.ts",
    "dev:debug": "node scripts/kill-ports.js && cross-env NODE_ENV=development PORT=3000 node --inspect=0.0.0.0:9339 -r ts-node/register src/app.ts",
    "dev:debug:watch": "node scripts/kill-ports.js && cross-env NODE_ENV=development PORT=3000 nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'node --inspect=0.0.0.0:9339 -r ts-node/register' src/app.ts",
    "dev:watch": "node scripts/kill-ports.js && cross-env NODE_ENV=development PORT=3000 nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "dev:bg": "node scripts/kill-ports.js && cross-env NODE_ENV=development PORT=3000 nohup ts-node src/app.ts > server.log 2>&1 &",
    "quick-start": "cross-env NODE_ENV=development PORT=3000 ts-node src/quick-start.ts",
    "build": "tsc --noEmitOnError false --skipLibCheck || true && npm run copy:dictionaries",
    "copy:dictionaries": "mkdir -p dist/config && cp -r src/config/ai-dictionaries dist/config/",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "test:ai-services": "jest tests/services/ai",
    "test:watch": "jest --watch",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit",
    "security:scan": "npx semgrep --config=auto src/",
    "security:mobile": "jest ../client/src/tests/mobile/security/",
    "security:full": "npm run security:audit && npm run security:scan",
    "migrate": "ts-node src/scripts/migrate.ts",
    "init-db": "ts-node src/scripts/initDatabase.ts",
    "db:create": "ts-node src/scripts/createDatabase.ts",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "db:migrate:undo": "ts-node src/scripts/migrateUndo.ts",
    "db:reset": "ts-node src/scripts/resetDatabase.ts",
    "db:drop": "ts-node src/scripts/dropDatabase.ts",
    "db:docs": "ts-node src/scripts/generate-db-docs.ts",
    "lint": "eslint . --ext .ts",
    "check:alignment": "ts-node src/scripts/check-alignment.ts",
    "check:alignment:fix": "ts-node src/scripts/check-alignment.ts --fix",
    "check:alignment:table": "ts-node src/scripts/check-alignment.ts --table=",
    "fix:generated": "ts-node src/scripts/fix-generated-files.ts",
    "test:crud": "ts-node src/scripts/test-crud-factory.ts",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "docs:update": "node scripts/update-swagger-docs.js",
    "docs:check": "node scripts/check-api-docs-quality.js",
    "docs:generate": "npm run docs:update && npm run docs:check",
    "mock:start": "node scripts/start-mock-server.js",
    "mock:basic": "node scripts/start-mock-server.js --basic",
    "mock:advanced": "node scripts/start-mock-server.js --advanced",
    "mock:dev": "node scripts/start-mock-server.js --advanced --verbose",
    "mock:swagger": "node scripts/start-swagger-mock.js",
    "mock:swagger:v2": "node scripts/start-swagger-mock-v2.js",
    "mock:swagger:port": "node scripts/start-swagger-mock.js --port=3010",
    "mock:swagger:test": "node scripts/test-swagger-mock.js",
    "import:school-readiness": "ts-node src/scripts/import-school-readiness-simple.ts",
    "import:school-readiness-full": "ts-node src/scripts/import-school-readiness-complete.ts",
    "verify:school-readiness": "ts-node src/scripts/verify-school-readiness-data.ts",
    "test:video-processing": "ts-node src/scripts/test-video-processing.ts",
    "mock:4000": "cross-env NODE_ENV=development MOCK_PORT=4000 ts-node src/mock/start-mock-server.ts",
    "mock:4000:prod": "cross-env NODE_ENV=production MOCK_PORT=4000 node dist/mock/start-mock-server.js"
  },
  "keywords": [
    "kindergarten",
    "management",
    "system",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Kindergarten Development Team",
  "license": "ISC",
  "dependencies": {
    "@alicloud/facebody20191230": "^1.2.12",
    "@alicloud/kms-sdk": "^1.1.0",
    "@alicloud/openapi-client": "^0.4.15",
    "@element-plus/icons-vue": "^2.3.1",
    "@faker-js/faker": "^8.4.1",
